{
	"name": "fml",
	"version": "1.0.0",
	"description": "Get a random fml quote!",
	"main": "./dist/cjs/index.js",
	"types": "./dist/",
	"exports": {
		".": [
			{
				"require": "./dist/cjs/index.js",
				"import": "./dist/esm/index.js"
			},
			"./dist/cjs/index.js"
		],
		"./dist/esm": "./dist/esm/index.js"
	},
	"scripts": {
		"test": "node test/index.cjs && eslint . --fix",
		"prettier": "prettier . --write",
		"build": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
		"prebuild": "rimraf dist && rimraf typings"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/1chiSensei/fml.js.git"
	},
	"keywords": [
		"fml"
	],
	"author": "Yoshida Tomio <https://github.com/1chiSensei>",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/1chiSensei/fml.js/issues"
	},
	"homepage": "https://github.com/1chiSensei/fml.js#readme",
	"devDependencies": {
		"@types/node": "^15.6.1",
		"@types/node-fetch": "^2.5.10",
		"@typescript-eslint/eslint-plugin": "^4.25.0",
		"@typescript-eslint/parser": "^4.25.0",
		"eslint": "^7.27.0",
		"eslint-config-marine": "^9.0.6",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"prettier": "^2.3.0",
		"rimraf": "^3.0.2",
		"typescript": "^4.3.2"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.9",
		"node-fetch": "^2.6.1"
	},
	"eslintConfig": {
		"extends": "marine/prettier/node"
	},
	"prettier": {
		"printWidth": 120,
		"useTabs": true,
		"singleQuote": true,
		"quoteProps": "as-needed",
		"trailingComma": "all",
		"endOfLine": "lf",
		"tabWidth": 8
	},
	"files": [
		"dist"
	]
}
